<p id="notice"><%= notice %></p>

<h1>Availability Blocks</h1>

<table>
  <thead>
    <tr>
      <th>Coach</th>
      <th>Day</th>
      <th>Start time</th>
      <th>End time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @availability_blocks.each do |availability_block| %>
      <tr>
        <td><%= availability_block.coach_id %></td>
        <td><%= availability_block.day %></td>
        <td><%= availability_block.start_time %></td>
        <td><%= availability_block.end_time %></td>
        <td><%= link_to 'Show', availability_block %></td>
        <td><%= link_to 'Edit', edit_availability_block_path(availability_block) %></td>
        <td><%= link_to 'Destroy', availability_block, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Availability Block', new_availability_block_path %>
