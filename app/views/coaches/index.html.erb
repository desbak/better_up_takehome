<p id="notice"><%= notice %></p>

<h1>Select A Coach To Make An Appointment</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Time zone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coaches.each do |coach| %>
      <tr>
        <td><%= coach.name %></td>
        <td><%= coach.time_zone %></td>
        <td><%= link_to 'View Availability', coach %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
